	else if(name_check.test(orderFrm.person_nm.value)==true){
		alert("받으실 분의 이름은 영어, 한글만 입력하셔야 합니다.");
		orderFrm.person_nm.focus();
		return false;
	}
	else if(number_check.test(orderFrm.person_hp1.value)==true || orderFrm.person_hp1.value.length<3 || orderFrm.person_hp1.value=="0"){
		alert("휴대폰 번호는 3자리 이상의 숫자만 입력하셔야 합니다.");
		orderFrm.person_hp1.focus();
		return false;
	}
	else if(number_check2.test(orderFrm.person_hp2.value)==true || orderFrm.person_hp2.value.length<3 || orderFrm.person_hp2.value=="0"){
		alert("휴대폰 번호는 3자리 이상의 숫자만 입력하셔야 합니다.");
		orderFrm.person_hp2.focus();
		return false;
