		orderFrm.person_hp3.focus();
		return false;
	}
	else if(orderFrm.person_htel1.value!=""&&(number_check.test(orderFrm.person_htel1.value)==true || orderFrm.person_htel1.value.length<3 || orderFrm.person_htel1.value=="0")){
