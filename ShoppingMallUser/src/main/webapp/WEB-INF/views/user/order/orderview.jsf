		alert("휴대폰 번호는 3자리 이상의 숫자만 입력하셔야 합니다.");
		orderFrm.chp3.focus();
		return false;
	}
	else if(orderFrm.ctel1.value!=""&&(number_check.test(orderFrm.ctel1.value)==true || orderFrm.ctel1.value.length<3 || orderFrm.ctel1.value=="0")){
		alert("전화 번호는 3자리 이상의 숫자만 입력하셔야 합니다.");
		orderFrm.ctel1.focus();
