});
function change(){
	var ck = document.getElementsByName("agree");
	var all_ck = document.getElementById("all_agree");
	var w = true;
	var count = 0;
	for(var p=0;p<ck.length;p++){
	if(ck[p].checked==false){
		all_ck.checked=false;
		w=false;
		break;
	}	
	else{
		count++;
	}
	}
	if(count==ck.length){
		all_ck.checked=true;
	}
}
function paycheck(){
