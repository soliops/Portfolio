	}
}
function ship_check(){
	if(orderFrm.ship.value=="선불"){
		orderFrm.ship_pay.value="5000";		
		document.getElementById("ship_sum").innerHTML="<fm:formatNumber value='5000'/>";
		document.getElementById("total_sum").innerHTML="<fm:formatNumber value='${list.total+5000}'/>"
	}
	else{
		orderFrm.ship_pay.value="0";
		document.getElementById("ship_sum").innerHTML="<fm:formatNumber value='0'/>";
		document.getElementById("total_sum").innerHTML="<fm:formatNumber value='${list.total}'/>"
	}
}
function all_check(){
	var name_check = /[^a-zA-Z가-힣]/g;
	var number_check = /[^0-9]/;
	var number_check2 = /[^0-9]\d{2,3}/;
	var email_check =/^[0-9a-zA-Z]([-_.]?[0-9a-zA-Z])*@[0-9a-zA-Z]([-_.]?[0-9a-zA-Z])*.[a-zA-Z]{2,3}$/g;
